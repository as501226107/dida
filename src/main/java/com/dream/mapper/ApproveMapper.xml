<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dream.mapper.ApproveMapper">


    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.dream.bean.Approve">
        <id column="id" property="id" />
        <result column="user_id" property="userId" />
        <result column="approveDate" property="approveDate" />
        <result column="comment" property="comment" />
        <result column="apply_id" property="applyId" />
        <result column="approveValue" property="approveValue" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, user_id, approveDate, comment, apply_id, approveValue
    </sql>

</mapper>
